    "bcryptjs": "^3.0.2",
    "cookie": "^1.0.2",
    "dotenv": "^17.2.3",
    "jose": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.525.0",
    "next": "15.4.10",
    "next-intl": "^4.7.0",
    "nuqs": "^2.4.3",
    "pg": "^8.16.3",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "sonner": "^2.0.6",
    "stripe": "^18.3.0",
