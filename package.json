  "pnpm": {
    "overrides": {
      "@types/react": "19.1.9",
      "@types/react-dom": "19.1.7"
    }
  },
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.0.0"
  },
  "volta": {
    "node": "22.21.1"
